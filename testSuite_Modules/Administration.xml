<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuite">

	<parameter name="module" value="${module}" />
	<parameter name="client" value="${client}" />
	<parameter name="env" value="${env}" />

	<listeners>
		<listener class-name="com.listeners.ClientModuleSuiteFilter" />
	</listeners>


     <test name="AdministrationSecurity">
		<classes>
			 <class name="testsuit.adminSecurity.UserAccount_AddNewUserTest" />
			<class name="testsuit.adminSecurity.ManageUser_AddNewUserTest" />
			<class name="testsuit.adminSecurity.UserRole_PagePermissionTest" />
			<class name="testsuit.adminSecurity.UserRole_SpecialPermissionTest" />
			<class name="testsuit.adminSecurity.UserRole_TemplatePermissionTest" />
			<class name="testsuit.adminSecurity.PasswordEncryptionTest" />  
		</classes>
	</test>
	
	<test name="AdministrationTemplate">
		<classes>
			 <class name="testsuit.AdministrationTemplate.AddNewTemplateTest" /> 
		</classes>
	</test>
	
	<test name="AdministrationConfiguration">
		<classes>
			<!--<class name="testsuit.AdministrationConfiguration.AddNewDropDownTest" />-->
			<class name="testsuit.AdministrationConfiguration.PermitAgenciesTest" />
			<class name="testsuit.AdministrationConfiguration.OperatingCompaniesTest" />
			<class name="testsuit.AdministrationConfiguration.ContractCompaniesTest" />
			<class name="testsuit.AdministrationConfiguration.PredefinedClausesTest" />
			<class name="testsuit.AdministrationConfiguration.GroupListTest" />
		</classes>
	</test>
	
	<test name="Administration_IndexManager">
		<classes>
			 <class name="testsuit.Administration_IndexManager.IndexManagerTest" /> 
		</classes>
	</test>
	
	
	
	
</suite> <!-- Suite -->

